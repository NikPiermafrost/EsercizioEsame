@{
    ViewBag.Title = "Home Page";
}
@model List<EsercizioEsame.Models.Ticket>

<div class="jumbotron">
    <h1>Last 10 Tickets</h1>
</div>

<div class="row">
    @foreach (var item in Model)
    {
        <div class="col-md-4">
            <h2>@item.Request</h2>
        </div>
        <div class="col-md-4">
            <h2>@item.Description</h2>
        </div>
        <div class="col-md-4">
            <h2>@item.Answer</h2>
        </div>
    }
</div>